<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.uttab.halcondeoro">

    <application
            android:name=".AppHalcon"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.HalconDeOro">
        <activity android:name=".AgregarActivity"
        android:parentActivityName=".MainActivity"
        ></activity>
        <activity android:name=".MainActivity"
                  android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RadiosActivity"></activity>
        <activity android:name=".AmpliActivity"></activity>
        <activity android:name=".AntenaActivity"></activity>
        <activity android:name=".AccesActivity"></activity>

    </application>

</manifest>